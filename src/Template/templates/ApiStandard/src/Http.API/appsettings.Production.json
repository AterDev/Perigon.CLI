{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "Error"
    }
  },
  "Authentication": {
    "DefaultScheme": "Bearer",
    "Schemes": {
      "Bearer": {
        "ValidAudiences": "MyProjectName.AllFreedomPeople.Default",
        "ValidIssuer": "MyProjectName.HumanFreedom.Default",
        "Sign": "MyProjectName.FightForFreedomForever.Justice"
      }
    }
  },
  "LoginSecurityPolicy": {
    "PasswordLevel": 0,
    "IsNeedVerifyCode": false,
    "PasswordExpired": 12,
    "LoginRetry": 5,
    "SessionLevel": 0
  },
  "ConnectionStrings": {
    "CommandDb": "Server=localhost;Port=5432;Database=MyProjectName;User Id=postgres;Password=root;",
    "QueryDb": "Server=localhost;Port=5432;Database=MyProjectName;User Id=postgres;Password=root;",
    "Cache": "localhost:6379",
    "CacheInstanceName": "Prod"
  },
  "Smtp": {
    "Host": "",
    "Port": 25,
    "DisplayName": "",
    "From": "",
    "Username": "",
    "Password": "",
    "EnableSsl": true
  },
  "AllowedHosts": "*",
  "OTEL_RESOURCE_ATTRIBUTES": "service.name=MyProjectName,service.instance.id=Production",
  "OTEL_EXPORTER_OTLP_ENDPOINT": "http://localhost:4317",
  "OTEL_EXPORTER_OTLP_HEADERS": "x-otlp-api-key=your-api-key"
}
