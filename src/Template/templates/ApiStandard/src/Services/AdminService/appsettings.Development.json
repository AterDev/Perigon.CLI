{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Components": {
    "Cache": "Memory",
    "Database": "PostgreSQL",
    "UseCORS": true,
    "AuthType": "Jwt",
    "MQType": "None",
    "UseSMS": false,
    "UseSmtp": false,
    "UseAWSS3": false,
    "UseOpenAPI": true
  },
  "Authentication": {
    "Jwt": {
      "ValidAudiences": "MyProjectName.AllFreedomPeople.Default",
      "ValidIssuer": "MyProjectName.HumanFreedom.Default",
      "Sign": "MyProjectName.FightForFreedomForever.Justice",
      "ExpiredSecond": 7200
    }
  },
  "LoginSecurityPolicy": {
    "PasswordLevel": 0,
    "IsNeedVerifyCode": false,
    "PasswordExpired": 12,
    "LoginRetry": 5,
    "SessionLevel": 0
  },

  "ConnectionStrings": {
